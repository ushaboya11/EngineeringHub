def compareTriplets(a, b):
    alice_score = 0
    bob_score = 0
    
    for i in range(3):
        if a[i] > b[i]:
            alice_score += 1
        elif a[i] < b[i]:
            bob_score += 1
        # if equal, no points
    
    return [alice_score, bob_score]

if __name__ == '__main__':
    a = list(map(int, input().split()))
    b = list(map(int, input().split()))
    result = compareTriplets(a, b)
    print(result[0], result[1])
